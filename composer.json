{
    "name": "diverently/phpstan-livewire",
    "description": "Recognize Livewire computed properties",
    "type": "phpstan-extension",
    "license": "MIT",
    "require": {
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Diverently\\PhpstanLivewire\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Robert Cordes",
            "email": "robert@diverently.com"
        }
    ],
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "require-dev": {
        "livewire/livewire": "^2.12"
    }
}
